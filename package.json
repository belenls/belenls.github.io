  "bugs": {
    "url": "https://github.com/belenls/belenls.github.io/issues"
  },
  "homepage": "https://belensantamarina.com",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "babel-preset-minify": "^0.5.1",
    "sass": "^1.35.1",
    "serve": "^12.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "babel-preset-minify"
    ]
  }
